*.jpif =	svn:keywords=Author Date HeadURL Id Revision
*.js =		svn:keywords=Author Date HeadURL Id Revision
*.jsp =		svn:keywords=Author Date HeadURL Id Revision
*.html =	svn:keywords=Author Date HeadURL Id Revision
*.properties =	svn:keywords=Author Date HeadURL Id Revision
*.script =	svn:keywords=Author Date HeadURL Id Revision
*.sh =		svn:keywords=Author Date HeadURL Id Revision;	svn:executable
*.sql =		svn:keywords=Author Date HeadURL Id Revision
*.tag =		svn:keywords=Author Date HeadURL Id Revision
*.xml =		svn:keywords=Author Date HeadURL Id Revision
*.xsd =		svn:keywords=Author Date HeadURL Id Revision

