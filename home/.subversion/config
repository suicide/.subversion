# Einstellungen, die in .subversion/config vorgenommen werden  muessen

[miscellany]
global-ignores = *.iml *.o *.lo *.la #*# .*.rej *.rej .*~ *~ .#* .DS_Store .classpath .idea .git .project .settings target
enable-auto-props = yes

[auto-props]
# Textdateien, in denen die Keywords ersetzt werden sollen
*.css =		svn:keywords=Author Date HeadURL Id Revision
